def solution(A):
    if len(A) == 0:
        return 1
    else:
        visit = [False]*(len(A) + 1)
        for i in A:
            visit[i-1] = True
        for i in range(len(visit)):
            if not visit[i]:
                return i+1

print solution([2,3,1,5])
print solution([1])
print solution([])
print solution([1,2])
print solution([1,2])
print solution([1,3])
print solution(range(1,10**5))
