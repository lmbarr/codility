def solution(A, K):
    if len(A) == 0:
        return []
    else:
        shift = K % len(A)
        return A[-shift:] + A[0:-shift]


print solution([3, 8, 9, 7, 6] , 1)
print solution([3, 8, 9, 7, 6] , 3)
print solution([3, 8, 9, 7, 6] , 10)
print solution([3, 8, 9, 7, 6] , 5)
print solution([3, 8, 9, 7, 6] , 0)
print solution([3, 8, 9, 7, 6, 0] , 5)
print solution([3] , 10000)
print solution([3,9] , 1)
print solution([], 3)
