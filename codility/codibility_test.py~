import random
def solution(A):
    # write your code in Python 2.7
    maximo = max(A)
    if maximo < 0:
        return 1
    else:
        l = [False] * (len(A) + 1)
        for elemet in A:
            if elemet > 0 and elemet <= len(A):
                l[elemet] = True
        for i in range(1, len(l) + 1):
            if not(l[i]):
                return i
                
print solution([-12,0,12])
print solution([1,0,2])
print solution([100,22,0,1,123])
lista = range(11)
random.shuffle(lista)
print solution(lista)
